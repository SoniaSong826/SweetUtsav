{
  "expo": {
    "name": "sweetutsav",
    "slug": "sweetutsav",
    "version": "2.3.0",
    "orientation": "portrait",
    "platforms": ["ios", "android"],
    "icon": "./App/assets/icon.png",
    "splash": {
      "image": "./App/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sonia.sweetUtsav",
      "buildNumber": "2.3.0"
    },
    "android": {
      "package": "com.sonia.sweetUtsav",
      "versionCode": 8,
      "adaptiveIcon": {
        "foregroundImage": "./App/assets/icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "../assets/favicon.png"
    }
  }
}

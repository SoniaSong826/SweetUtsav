{
  "expo": {
    "name": "sweetutsav",
    "slug": "sweetutsav",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./App/assets/icon.png",
    "splash": {
      "image": "./App/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier":"com.sonia.sweetUtsav",
      "buildNumber":"1.0.0"
    },
    "android": {
      "package":"com.sonia.sweetUtsav",
      "versionCode":3,
      "adaptiveIcon": {
        "foregroundImage": "./App/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "../assets/favicon.png"
    }
  }
}
